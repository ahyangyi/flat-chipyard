{
    "name": "Xilinx XF_BLAS Matrix Transposer",
    "description": [
        "A dense matrix transposer test"
    ],
    "flow": "vitis",
    "platform_whitelist": [
        "aws"
    ],
    "platform_blacklist": [
        "zc"
    ],
    "runtime": [
        "OpenCL"
    ], 
    "launch": [
    {
        "name": "generic launch for all flows",
        "cmd_args": "BUILD/transp.xclbin transp 256 512 A0 B0"
    }
    ],
    "host": {
        "host_exe": "host.exe",
        "compiler": {
            "sources": [
                "PROJECT/src/sw/transp_main.cpp", 
                "PROJECT/src/sw/xcl2/xcl2.cpp"
            ], 
            "includepaths": [
                "PROJECT/src", 
                "PROJECT/include/hw/xf_blas/helpers/utils", 
                "PROJECT/include/", 
                "PROJECT/include/sw", 
                "PROJECT/include/sw/xcl2"
            ], 
            "options": [
                "-O3 -Wall -Wno-unknown-pragmas -Wno-unused-label -Wno-uninitialized -Wno-unused-value"
            ],
            "symbols":[
                "BLAS_ddrWidth=16",
                "BLAS_numKernels=1",
                "BLAS_dataType=float",
                "BLAS_argPipeline=2",
                "BLAS_instructionSizeBytes=64",
                "BLAS_argInstrWidth=1",
                "BLAS_dataEqIntType=uint_32",
                "BLAS_numInstr=2",
                "BLAS_transpBlocks=1",
                "BLAS_gemvmGroups=1",
                "BLAS_runTransp=1"
            ]
        }
    },
    "v++": {
        "compiler": {
            "includepaths": [
                "PROJECT/include/hw", 
                "PROJECT/include/hw/xf_blas", 
                "PROJECT/include/hw/xf_blas/gemm", 
                "PROJECT/include/hw/xf_blas/helpers/utils", 
                "PROJECT/include"
            ],
            "symbols":[
                "BLAS_ddrWidth=16",
                "BLAS_numKernels=1",
                "BLAS_dataType=float",
                "BLAS_argPipeline=2",
                "BLAS_instructionSizeBytes=64",
                "BLAS_argInstrWidth=1",
                "BLAS_dataEqIntType=uint_32",
                "BLAS_numInstr=2",
                "BLAS_transpBlocks=1",
                "BLAS_gemvmGroups=1",
                "BLAS_runTransp=1"
            ]
        }
    },
    "containers": [
        {
            "name" : "transp",
            "accelerators": [
                {
                    "name": "blasKernel", 
                    "location": "PROJECT/src/hw/kernel.cpp" 
                }
            ]
        }
    ],
    "testinfo": {
        "disable": false,
        "jobs": [
            {
                "index": 0,
                "dependency": [],
                "env": "",
                "cmd": "",
                "max_memory_MB": 32768,
                "max_time_min": 520
            }
        ],
        "targets": [
            "vitis_sw_emu", 
            "vitis_hw_emu",
            "vitis_hw_build",
            "vitis_hw_run"
        ],
        "category": "canary"
    }
}

