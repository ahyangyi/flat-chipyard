{
    "name": "Array Partition (HLS C/C++ Kernel)", 
    "description": [
        "This is a simple example of matrix multiplication (Row x Col) to demonstrate how to achieve better performance by array partitioning, using HLS kernel in Vitis Environment."
    ],
    "flow": "vitis",
    "keywords": [
        "#pragma HLS ARRAY_PARTITION", 
        "complete"
    ], 
    "key_concepts": [
        "Kernel Optimization", 
        "HLS C Kernel", 
        "Array Partition"
    ], 
    "os": [
        "Linux"
    ], 
    "runtime": [
        "OpenCL"
    ], 
    "host": {
        "host_exe": "array_partition",
        "compiler": {
            "sources": [
                "REPO_DIR/common/includes/xcl2/xcl2.cpp",
                "./src/host.cpp"
            ], 
            "includepaths": [
                "REPO_DIR/common/includes/xcl2"
            ]
        }
    }, 
    "containers": [
        {
            "accelerators": [
                {
                    "name": "matmul", 
                    "location": "src/matmul.cpp"
                }, 
                {
                    "name": "matmul_partition", 
                    "location": "src/matmul_partition.cpp"
                }
            ], 
            "name": "matmul"
        }
    ],
    "launch": [
        {
            "cmd_args": "BUILD/matmul.xclbin", 
            "name": "generic launch for all flows"
        }
    ], 
    "contributors": [
        {
            "url": "http://www.xilinx.com", 
            "group": "Xilinx"
        }
    ],
    "testinfo": {
        "profile": "no",
        "disable": false,
        "jobs": [
            {
                "index": 0,
                "dependency": [],
                "env": "",
                "cmd": "",
                "max_memory_MB": 32768,
                "max_time_min": 300
            }
        ],
        "targets": [
            "vitis_sw_emu",
            "vitis_hw_emu",
            "vitis_hw"
        ],
        "category": "canary"
    }   
}
