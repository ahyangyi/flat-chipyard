{
    "name": "RTL Stream Kernel to Kernel and Memory Mapped (RTL Kernel)", 
    "description": [
        "This example uses two simple increment RTL kernels connected to each other via stream interface and connected to host via memory mapped C++ data mover kernels"
    ], 
    "flow": "vitis",
    "config_make": "config.mk",
    "platform_whitelist": [
        "u200_qdma"
    ],  
   "ndevice": [
      	"vck",
        "zc706"
       ], 
    "platform_blacklist": [
      	"vck",
        "zc706"
       ], 
    "contributors": [
        {
            "url": "http://www.xilinx.com", 
            "group": "Xilinx"
        }
    ], 
    "launch": [
        {
            "cmd_args": "BUILD/myadder.xclbin", 
            "name": "generic launch for all flows"
        }
    ], 
    "gui": false, 
    "host": {
        "linker": {
            "options": [
                "-pthread"
            ]
        }, 
        "host_exe": "rtl_streaming_k2k_mm",
        "compiler": {
            "sources": [
                "REPO_DIR/common/includes/xcl2/xcl2.cpp", 
                "src/host.cpp"
            ], 
            "includepaths": [
                "REPO_DIR/common/includes/xcl2"
            ]
        }
    }, 
    "key_concepts": [
        "Read/Write Stream", 
        "RTL Kernel"
    ], 
    "keywords": [
        "ap_axiu",
	"hls::stream" 
    ], 
    "runtime": [
        "OpenCL"
    ], 
    "os": [
        "Linux"
    ], 
    "targets": [
        "hw", 
        "hw_emu"
    ], 
    "containers": [
        {
            "accelerators": [
                {
                    "kernel_type": "RTL", 
                    "name": "myadder1"
                }, 
                {
                    "kernel_type": "RTL", 
                    "name": "myadder2"
		},
                {
                    "name": "krnl_mm2s", 
                    "location": "src/krnl_mm2s"
                }, 
                {
                    "name": "krnl_s2mm", 
                    "location": "src/krnl_s2mm"
                } 
            ], 
            "name": "myadder",
	    "ldclflags": "--config PROJECT/myadder.ini"
        }
    ], 
    "testinfo": {
        "disable": false,
        "jobs": [
            {
                "index": 0,
                "dependency": [],
                "env": "",
                "cmd": "",
                "max_memory_MB": 32768,
                "max_time_min": 300
            }
        ],
        "targets": [
            "vitis_hw_emu",
            "vitis_hw"
        ],
        "category": "canary"
    },
    "output_files": "tmp_kernel_pack* packaged_kernel* _x* *_ex project* pinfo.json *.xo"
}
