{
    "gui": true, 
    "name": "Xilinx MC European Engine Benchmark", 
    "description": [
        "Xilinx XF_FINTECH Test"
    ], 
    "flow": "vitis", 
    "platform_type": "pcie", 
    "platform_whitelist": [
        "aws"
    ], 
    "platform_blacklist": [
        "zc",
        "qdma",
        "vck"
    ],  
    "runtime": [
        "OpenCL"
    ], 
    "launch": [
        {
            "cmd_args": " -xclbin BUILD/kernel_mc.xclbin", 
            "name": "generic launch for all flows"
        }
    ], 
    "host": {
        "host_exe": "test.exe", 
        "compiler": {
            "sources": [
                "PROJECT/src/test.cpp",
                "PROJECT/src/xcl2.cpp"
            ], 
            "includepaths": [
                "PROJECT/include", 
                "PROJECT/src"
            ], 
            "options": [
                "-O3" 
            ]
        }
    }, 
    "v++": {
        "compiler": {
            "includepaths": [
                "PROJECT/include", 
                "PROJECT/src"
            ]
        }
    }, 
    "containers": [
        {
            "accelerators": [
                {
                    "location": "PROJECT/src/kernel_mc.cpp", 
                    "clflags": " -D DPRAGMA", 
                    "name": "kernel_mc", 
                    "num_compute_units": "3", 
                    "compute_units": [
                        {
                            "slr": "SLR0", 
                            "arguments": [
                                {
                                    "name": "m_axi_gmem", 
                                    "memory": "DDR[3]"
                                }
                            ]
                        }, 
                        {
                            "slr": "SLR1", 
                            "arguments": [
                                {
                                    "name": "m_axi_gmem", 
                                    "memory": "DDR[0]"
                                }
                            ]
                        }, 
                        {
                            "slr": "SLR2", 
                            "arguments": [
                                {
                                    "name": "m_axi_gmem", 
                                    "memory": "DDR[1]"
                                }
                            ]
                        }
                    ]
                }
            ], 
            "name": "kernel_mc",
            "ldclflags": "--config PROJECT/conn_vu9p.ini --kernel_frequency 300"
        }
    ], 
    "testinfo": {
        "disable": false, 
        "jobs": [
            {
                "index": 0, 
                "dependency": [], 
                "env": "", 
                "cmd": "", 
                "max_memory_MB": 32768, 
                "max_time_min": {
                    "vitis_hw_build": 1200, 
                    "vitis_hw_emu": 300, 
                    "vitis_sw_emu": 60, 
                    "vitis_hw_run": 10
                }
            }
        ], 
        "targets": [
            "vitis_sw_emu", 
            "vitis_hw_emu", 
            "vitis_hw_build",
            "vitis_hw_run"
        ], 
        "category": "canary"
    }
}
