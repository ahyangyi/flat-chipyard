{
    "name": "Stream Multiple Compute Units",
    "description": [
        "This is a simple vadd Kernel design in which multiple compute units of kernel are directly communicating with host using stream interfaces."
    ], 
    "flow": "vitis",
    "keywords": [
        "cl_stream", 
        "CL_STREAM_EOT", 
        "nk"
    ], 
    "key_concepts": [
        "Read/Write Stream", 
        "Create/Release Stream"
    ], 
    "platform_whitelist": [
        "u200_qdma"
    ],  
    "ndevice": [
        "u250",
        "zc",
        "vck",
        "xdma",
        "qep",	
        "aws",
        "samsung"
    ], 
    "platform_blacklist": [
        "u250",
        "zc",
        "vck",
        "xdma",
        "qep",	
        "aws",
        "samsung"
    ], 
    "os": [
        "Linux"
    ], 
    "runtime": [
        "OpenCL"
    ], 
    "host": {
        "host_exe": "streaming_multi_cus",
        "compiler": {
            "sources": [
                "REPO_DIR/common/includes/xcl2/xcl2.cpp",
                "./src/host.cpp"
            ], 
            "includepaths": [
                "REPO_DIR/common/includes/xcl2"
            ]
        }
    }, 
    "containers": [
        {
            "accelerators": [
                {
                    "location": "src/krnl_stream_vadd.cpp", 
                    "name": "krnl_stream_vadd", 
                    "num_compute_units": "4:vadd_1.vadd_2.vadd_3.vadd_4"
                }
            ], 
            "name": "krnl_stream_vadd"
        }
    ],
    "launch": [
        {
            "cmd_args": "BUILD/krnl_stream_vadd.xclbin", 
            "name": "generic launch for all flows"
        }
    ], 
    "contributors": [
        {
            "url": "http://www.xilinx.com", 
            "group": "Xilinx"
        }
    ],
    "testinfo": {
        "disable": false,
        "jobs": [
            {
                "index": 0,
                "dependency": [],
                "env": "",
                "cmd": "",
                "max_memory_MB": 32768,
                "max_time_min": 300
            }
        ],
        "targets": [
            "vitis_sw_emu",
            "vitis_hw_emu",
            "vitis_hw"
        ],
        "category": "canary"
    }
}
