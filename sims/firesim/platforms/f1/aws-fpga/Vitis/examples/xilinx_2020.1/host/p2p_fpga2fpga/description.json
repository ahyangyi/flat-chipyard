{
    "name": "P2P FPGA to FPGA Example", 
    "description": [
        "This is simple example to explain P2P transfer between two FPGA devices."
    ],
    "flow": "vitis",
    "keywords": [
        "XCL_MEM_EXT_P2P_BUFFER"
    ],
    "key_concepts": [
        "P2P",
        "Multi-FPGA Execution", 
        "XDMA"
    ],
    "num_devices": "2",
    "platform_type": "pcie",
    "os": [
        "Linux"
    ],
    "platform_whitelist": [
        "samsung_u2"
    ],
    "ndevice": [
        "zc",
        "201910",
        "2018",
        "vck",
        "aws",
        "u50"
    ],
    "platform_blacklist": [
        "zc",
        "201910",
        "2018",
        "vck",
        "aws",
        "u50"
    ],
     "runtime": [
        "OpenCL"
    ], 
    "launch": [
        {
            "cmd_args": "-x1 BUILD/krnl_mmult.xclbin -x2 BUILD/krnl_madd.xclbin", 
            "name": "generic launch for all flows"
        }
    ], 
    "host": {
        "host_exe": "p2p_fpga2fpga",
        "compiler": {
            "sources": [
                "REPO_DIR/common/includes/xcl2/xcl2.cpp",
                "REPO_DIR/common/includes/cmdparser/cmdlineparser.cpp",
                "REPO_DIR/common/includes/logger/logger.cpp",
                "src/host.cpp"
            ], 
            "includepaths": [
                "REPO_DIR/common/includes/xcl2",
                "REPO_DIR/common/includes/cmdparser",
                "REPO_DIR/common/includes/logger"
            ]
        }
    },  
    "containers": [
        {
            "accelerators": [
                {
                    "container" : "krnl_mmult",
                    "name": "krnl_mmult", 
                    "location": "src/krnl_mmult.cpp"
                }
            ], 
            "name": "krnl_mmult"
        },
        {
            "accelerators": [
                {
                    "container" : "krnl_madd",
                    "name": "krnl_madd", 
                    "location": "src/krnl_madd.cpp"
                }
            ],
            "name": "krnl_madd"
        }
    ],
    "contributors": [
        {
            "url": "http://www.xilinx.com", 
            "group": "Xilinx"
        }
    ],
    "testinfo": {
        "disable": false,
        "jobs": [
            {
                "index": 0,
                "dependency": [],
                "env": "",
                "cmd": "",
                "max_memory_MB": 32768,
                "max_time_min": 300
            }
        ],
        "targets": [
            "vitis_sw_emu",
            "vitis_hw_emu",
            "vitis_hw"
        ],
        "category": "canary"
    }
}
