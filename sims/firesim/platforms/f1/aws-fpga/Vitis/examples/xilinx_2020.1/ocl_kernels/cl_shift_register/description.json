{
    "name": "Shift Register (OpenCL Kernel)", 
    "description": [
        "This example demonstrates how to shift values in registers in each clock cycle"
    ],
    "flow": "vitis",
    "keywords": [
        "xcl_array_partition", 
        "getProfilingInfo"
    ], 
    "key_concepts": [
        "Kernel Optimization", 
        "Shift Register", 
        "FIR"
    ], 
    "os": [
        "Linux"
    ], 
    "runtime": [
        "OpenCL"
    ], 
    "host": {
        "host_exe": "cl_shift_register",
        "compiler": {
            "sources": [
                "REPO_DIR/common/includes/xcl2/xcl2.cpp",
                "./src/host.cpp"
            ], 
            "includepaths": [
                "REPO_DIR/common/includes/xcl2"
            ]
        }
    }, 
    "containers": [
        {
            "accelerators": [
                {
                    "name": "fir_naive", 
                    "location": "src/fir.cl"
                }, 
                {
                    "name": "fir_shift_register", 
                    "location": "src/fir.cl"
                }
            ], 
            "name": "fir"
        }
    ], 
    "launch": [
        {
            "cmd_args": "BUILD/fir.xclbin", 
            "name": "generic launch for all flows"
        }
    ], 
    "match_ini": "false"	,
    "contributors": [
        {
            "url": "http://www.xilinx.com", 
            "group": "Xilinx"
        }
    ],
    "testinfo": {
        "disable": false,
        "jobs": [
            {
                "index": 0,
                "dependency": [],
                "env": "",
                "cmd": "",
                "max_memory_MB": 32768,
                "max_time_min": 300
            }
        ],
        "targets": [
            "vitis_sw_emu",
            "vitis_hw_emu",
            "vitis_hw"
        ],
        "category": "canary"
    }
}
