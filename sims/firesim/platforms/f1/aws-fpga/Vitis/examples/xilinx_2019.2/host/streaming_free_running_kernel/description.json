{
    "name": "Stream Free Running Kernel (HLS C/C++)", 
    "description": [
        "This is simple example which demonstrate how to use and configure a free running kernel."
    ],
    "keywords": [
        "ap_ctrl_none",
        "stream_connect" 
    ], 
    "key_concepts": [
        "Free Running Kernel" 
    ], 
    "ndevice": [
        "xdma",
	"xilinx_u250_qep",
	"aws",
        "samsung",
	"zc706",
	"zc702"
    ],
    "os": [
        "Linux"
    ], 
    "runtime": [
        "OpenCL"
    ], 
    "host": {
        "host_exe": "host", 
        "compiler": {
            "sources": [
                "REPO_DIR/common/includes/xcl2"
            ], 
            "includepaths": [
                "REPO_DIR/common/includes/xcl2"
            ]
        }
    }, 
    "containers": [
        {
            "accelerators": [
                {
                    "name": "mem_read", 
                    "location": "src/mem_read.cpp"
                },
                {
                    "name": "increment", 
                    "location": "src/increment.cpp"
                },
                {
                    "name": "mem_write", 
                    "location": "src/mem_write.cpp"
                }
            ], 
            "name": "krnl_incr",
	    "ldclflags": "--config PROJECT/krnl_incr.ini"
        }
    ],
    "launch": [
        {
            "cmd_args": "BUILD/krnl_incr.xclbin", 
            "name": "generic launch for all flows"
        }
    ], 
    "contributors": [
        {
            "url": "http://www.xilinx.com", 
            "group": "Xilinx"
        }
    ] 
}
