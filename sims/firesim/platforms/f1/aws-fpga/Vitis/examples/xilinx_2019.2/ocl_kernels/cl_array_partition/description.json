            ]
        }
    }, 
    "containers": [
        {
            "accelerators": [
                {
                    "name": "matmul", 
                    "location": "src/matmul.cl"
                }, 
                {
                    "name": "matmul_partition", 
                    "location": "src/matmul.cl"
                }
            ], 
            "name": "matmul"
        }
    ], 
    "launch": [
        {
            "cmd_args": "BUILD/matmul.xclbin", 
            "name": "generic launch for all flows"
        }
    ], 
    "output_files": "output.png", 
    "contributors": [
