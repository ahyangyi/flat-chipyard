{
    "name": "Xilinx AML",
    "description": [
        "Anti Money Laundering checking against deny-lists"
    ],
    "flow": "vitis",
    "platform_type": "pcie",
    "platform_whitelist": [
        "aws-vu9p-f1"
    ],
    "platform_blacklist": [
        "_u2_",
        "_u25_",
        "u30",
        "u50",
        "u200",
        "u280",
        "u250",
        "zc",
        "vck",
        "nodma"
    ],
    "runtime": [
        "OpenCL"
    ], 
    "launch": [
        {
            "cmd_args": " -xclbin BUILD/aml.xclbin -d PROJECT/data -c 0",
            "name": "generic launch for all flows"
        }
    ],
    "config_make": "./config.mk",
    "host": {
        "host_exe": "test_aml",
        "compiler": {
            "sources": [
                "PROJECT/host/test.cpp",
                "PROJECT/host/xcl2.cpp",
                "PROJECT/host/swift_mt103_check.cpp"
            ],
            "includepaths": [
                "PROJECT/host"
            ],
            "options": "-O3"
        }
    },
    "v++": {
        "compiler": {
            "includepaths": [
                "PROJECT/kernel"
            ]
        }
    },
    "containers": [
        {
            "accelerators": [
                {
                    "location": "PROJECT/kernel/fuzzy_kernel_1.cpp",
                    "name": "fuzzy_kernel_1",
                    "compute_units": [
                        {
                            "arguments": [
                                {
                                    "name": "buf_in", 
                                    "memory": "PLRAM[0]"
                                },
                                {
                                    "name": "csv_part0", 
                                    "memory": "DDR[1]"
                                },
                                {
                                    "name": "csv_part1", 
                                    "memory": "DDR[1]"
                                },
                                {
                                    "name": "csv_part2", 
                                    "memory": "DDR[1]"
                                },
                                {
                                    "name": "csv_part3", 
                                    "memory": "DDR[1]"
                                },
                                {
                                    "name": "csv_part4", 
                                    "memory": "DDR[1]"
                                },
                                {
                                    "name": "csv_part5", 
                                    "memory": "DDR[1]"
                                },
                                {
                                     "name": "csv_part6", 
                                    "memory": "DDR[1]"
                                },
                                {
                                     "name": "csv_part7", 
                                    "memory": "DDR[1]"
                                },
                                {
                                     "name": "buf_out", 
                                    "memory": "PLRAM[0]"
                                }
                            ]
                        }
                    ]
                },
                {
                    "location": "PROJECT/kernel/fuzzy_kernel_2.cpp",
                    "name": "fuzzy_kernel_2",
                    "compute_units": [
                        {
                            "arguments": [
                                {
                                    "name": "buf_in", 
                                    "memory": "PLRAM[1]"
                                },
                                {
                                    "name": "csv_part0", 
                                    "memory": "DDR[2]"
                                },
                                {
                                    "name": "csv_part1", 
                                    "memory": "DDR[2]"
                                },
                                {
                                    "name": "csv_part2", 
                                    "memory": "DDR[2]"
                                },
                                {
                                    "name": "csv_part3", 
                                    "memory": "DDR[2]"
                                },
                                {
                                    "name": "csv_part4", 
                                    "memory": "DDR[2]"
                                },
                                {
                                    "name": "csv_part5", 
                                    "memory": "DDR[2]"
                                },
                                {
                                     "name": "csv_part6", 
                                    "memory": "DDR[2]"
                                },
                                {
                                     "name": "csv_part7", 
                                    "memory": "DDR[2]"
                                },
                                {
                                     "name": "buf_out", 
                                    "memory": "PLRAM[1]"
                                }
                            ]
                        }
                    ]
                }
            ],
            "frequency": 300,
            "name": "aml"
        }
    ],
    "testinfo": {
        "level_up": [
            {
                "hw_emu": "1"
            }
        ],
        "disable": false,
        "jobs": [
            {
                "index": 0,
                "dependency": [],
                "env": "",
                "cmd": "",
                "max_memory_MB": 32768,
                "max_time_min": {
                    "vitis_hw_build": 1500,
                    "vitis_hw_emu": 900,
                    "vitis_sw_emu": 60,
                    "vitis_hw_run": 10
                }
            }
        ],
        "targets": [
            "vitis_sw_emu",
            "vitis_hw_emu",
            "vitis_hw_build",
            "vitis_hw_run"
        ],
        "category": "canary"
    }
}
