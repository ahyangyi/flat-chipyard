{
    "name": "PLRAM Memory Access (C)", 
    "description": [
        "This example shows the usage of PLRAM and how to use it with simple matrix multiplication (Row x Col)."
    ],
    "flow": "vitis",
    "keywords": [
        "PLRAM"
    ], 
    "key_concepts": [
        "Vitis Memory Hierarchy", 
        "PLRAMs"
    ], 
    "platform_blacklist": [
        "_u25_",
        "u30",
        "zc",
        "vck", 
        "u50",
        "u55",
        "samsung",
        "_u2_",
        "nodma"    
    ], 
    "os": [
        "Linux"
    ], 
    "runtime": [
        "OpenCL"
    ], 
    "platform_type": "pcie",
    "host": {
        "host_exe": "plram_access",
        "compiler": {
            "sources": [
                "REPO_DIR/common/includes/xcl2/xcl2.cpp",
                "./src/host.cpp"
            ], 
            "includepaths": [
                "REPO_DIR/common/includes/xcl2"
            ]
        }
    }, 
    "containers": [
        {
            "accelerators": [
                {
                    "compute_units": [
                        {
                            "arguments": [
                                {
                                    "name": "a", 
                                    "memory": "PLRAM[0]"
                                }, 
                                {
                                    "name": "b", 
                                    "memory": "DDR[0]"
                                }, 
                                {
                                    "name": "c", 
                                    "memory": "PLRAM[1]"
                                }
                            ]
                        }
                    ], 
                    "name": "mmult", 
                    "location": "src/mmult.cpp"
                }
            ], 
            "name": "mmult"
        }
    ],
    "launch": [
        {
            "cmd_args": "BUILD/mmult.xclbin", 
            "name": "generic launch for all flows"
        }
    ], 
    "contributors": [
        {
            "url": "http://www.xilinx.com", 
            "group": "Xilinx"
        }
    ],
    "testinfo": {
        "disable": false,
        "profile": "no",
        "jobs": [
            {
                "index": 0,
                "dependency": [],
                "env": "",
                "cmd": "",
                "max_memory_MB": 32768,
                "max_time_min": 300
            }
        ],
        "targets": [
            "vitis_sw_emu",
            "vitis_hw_emu",
            "vitis_hw"
        ],
        "category": "canary"
    } 
}
